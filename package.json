{
  "name": "career-network",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port=4000",
    "start:all": "concurrently --kill-others-on-fail \"npx json-server db.json\" \"next dev --port=4000\"",
